    <template id="employee">
        <section class="s_features_grid pt48 pb24 o_colored_level" data-snippet="s_features_grid"
                 data-name="Features Grid" style="background-image: none;">
            <div class="container">
                <div class="row">
                    <!-- Column for chefs -->
                    <div class="col-lg-6 s_col_no_bgcolor pb24">
                        <t t-foreach="chefs" t-as="chef">
                            <div class="row">
                                <div class="col-lg-12 pb24 o_colored_level">
                                    <h2 class="o_default_snippet_text">Nhân viên nấu ăn</h2>
                                    <h5 class="o_default_snippet_text">Đầu bếp của chúng tôi</h5>
                                </div>
                                <div class="col-lg-12 pb24 o_colored_level">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <div class="col-lg-4 pb16 o_not_editable">
                                            <a class="link" t-att-href="'/detail-employee/' + str(chef.id)">
                                                <img alt="" t-attf-src="/web/image?model=hr.employee&amp;field=image_1920&amp;id=#{chef.id}"
                                                     class="img-fluid rounded-circle mx-auto o_editable_media" loading="lazy"/>
                                            </a>
                                        </div>
                                        <div class="s_features_grid_content col-lg-8">
                                            <a class="link" t-att-href="'/detail-employee/' + str(chef.id)">
                                                <h3 class="o_default_snippet_text">
                                                    <t t-esc="chef.name"/>
                                                </h3>
                                            </a>
                                            <p class="o_default_snippet_text">
                                                <t t-esc="chef.note"/>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </t>
                    </div>
                    <!-- Column for serves -->
                    <div class="col-lg-6 s_col_no_bgcolor pb24">
                        <t t-foreach="serves" t-as="serve">
                            <div class="row">
                                <div class="col-lg-12 pb24 o_colored_level" data-name="Box">
                                    <h2 class="o_default_snippet_text">Nhân viên phục vụ</h2>
                                    <h5 class="o_default_snippet_text">Nhân viên phục vụ của chúng tôi</h5>
                                </div>
                                <div class="col-lg-12 pb24 o_colored_level">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <div class="col-lg-4 pb16 o_not_editable">
                                            <a class="link" t-att-href="'/detail-employee/' + str(serve.id)">
                                                <img alt="" t-attf-src="/web/image?model=hr.employee&amp;field=image_1920&amp;id=#{serve.id}"
                                                     class="img-fluid rounded-circle mx-auto o_editable_media" loading="lazy"/>
                                            </a>
                                        </div>
                                        <div class="s_features_grid_content col-lg-8">
                                            <a class="link" t-att-href="'/detail-employee/' + str(serve.id)">
                                                <h3 class="o_default_snippet_text">
                                                    <t t-esc="serve.name"/>
                                                </h3>
                                            </a>
                                            <p class="o_default_snippet_text">
                                                <t t-esc="serve.note"/>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </t>
                    </div>
                </div>
            </div>
        </section>
    </template>

